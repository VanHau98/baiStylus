.anhhung


		.carousel-inner

			.carousel-item
				.row
					.animate-box
						margin-top 35px
						&:hover
							box-shadow 5px 0px 40px rgba(223,0,41,0.2)
						
						@media screen and ( max-width: 989px)
							display none
						
						.gtco-staff
							margin 0 auto
							img
								margin 0 auto
						
								width 250px
								height 300px
								&:hover
									box-shadow 5px 0px 40px rgba(0,127,84,0.2)
									transform scale(1.02)
								
							
							h4
								text-align center
								font-family cursive
								color red
							
							h5
								margin-top 15px
								text-align center
								font-family cursive
								color gray
							
							.cccc
